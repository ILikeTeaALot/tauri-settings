  "homepage": "https://github.com/harshkhandeparkar/tauri-settings#readme",
  "devDependencies": {
    "@tauri-apps/api": "^1.4.0",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "lodash": "^4.17.21",
    "ts-jest": "^29.1.1",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
