{
  "name": "tauri-settings",
  "version": "0.3.4",
  "description": "A user settings manager for Tauri inspired by electron-settings.",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p config/tsconfig.json",
    "test": "jest --config config/jest.config.js",
    "dev": "tsc -p config/tsconfig.json -w",
    "doc": "typedoc --options config/typedoc.js --tsconfig config/tsconfig.json",
    "npm-publish": "pnpm run test && pnpm run build && pnpm publish --no-git-checks"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/harshkhandeparkar/tauri-settings.git"
  },
  "keywords": [
    "tauri",
    "tauri-apps",
    "tauri-settings",
    "settings-manager"
  ],
  "author": "harshkhandeparkar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/harshkhandeparkar/tauri-settings/issues"
  },
  "homepage": "https://github.com/harshkhandeparkar/tauri-settings#readme",
  "devDependencies": {
    "@tauri-apps/api": "^1.5.3",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "ts-jest": "^29.1.2",
    "typedoc": "^0.24.8",
    "typescript": "^5.4.5"
  }
}
